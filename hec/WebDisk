10.10.144.96 62000(운영)
10.176.244.255 62000(개발)

SELECT DEL_DATE, (SELECT USER_NAME FROM USER_INFO UI WHERE UI.USER_ID=DEL_USER) DEL_USER_NAKE, DEL_USER, CONCAT(ROUND(SUM(FILE_SIZE) / 1024 / 1024 / 1024, 2), ' GB') FILE_SIZE_GB, SUM(FILE_COUNT) FILE_COUNT
FROM (
	SELECT DEL_USER, DEL_DATE, FILE_SIZE, FILE_COUNT
	FROM (
		SELECT DEL_USER, SUBSTR(REGDATE, 1, 8) DEL_DATE, SUM(ORG_FILE_SIZE) FILE_SIZE, COUNT(SFILE_ID) FILE_COUNT
		FROM DEL_SHARE_INFO
		WHERE ITEM_TYPE = 1
			/*AND DEL_USER='0801863'*/
		GROUP BY DEL_USER, SUBSTR(REGDATE, 1, 8)
	) t1
	UNION ALL
	SELECT DEL_USER, DEL_DATE, FILE_SIZE, FILE_COUNT
	FROM (
		SELECT DEL_USER, SUBSTR(DEL_DATE, 1, 8) DEL_DATE, SUM(LFILE_SIZE) FILE_SIZE, COUNT(SFILE_ID) FILE_COUNT
		FROM LOG_DEL_SHARE
		WHERE 1=1
			/*AND DEL_USER='0801863'*/
		GROUP BY DEL_USER, SUBSTR(DEL_DATE, 1, 8)
	) t1
) t1
GROUP BY DEL_DATE, DEL_USER
ORDER BY DEL_DATE, DEL_USER

