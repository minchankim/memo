51000107 현엔회

SELECT table_name, num_rows FROM user_tables 


SELECT * FROM RP0100


SELECT TO_CHAR(JOIN_YMDHMS,'YYYY') AS YYYY,
       COUNT(*) AS CNT
FROM   RP0100 A
GROUP  BY TO_CHAR(JOIN_YMDHMS,'YYYY')
ORDER  BY YYYY ASC


SELECT   SEGMENT_NAME, BYTES / 1024 / 1024 / 1024 GB, bytes

    FROM   USER_SEGMENTS

   WHERE   SEGMENT_TYPE = 'TABLE'

ORDER BY   bytes DESC;








select count(*)
  from jrm.rp0100
 where withdraw_yn = 'N'

